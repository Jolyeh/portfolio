<!DOCTYPE html>
<html class="no-js">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Portfolio - <%= user.nom %>
			<%= user.prenom %>
	</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<!-- Style -->
	<link rel="shortcut icon" href="favicon.ico">
	<link href='https://fonts.googleapis.com/css?family=Work+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
	<link href='https://fonts.googleapis.com/css?family=Playfair+Display:400,400italic,700italic,700' rel='stylesheet'
		type='text/css'>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css"
		integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A=="
		crossorigin="anonymous" referrerpolicy="no-referrer" />
	<link rel="stylesheet" href="css/animate.css">
	<link rel="stylesheet" href="css/flexslider.css">
	<link rel="stylesheet" href="css/icomoon.css">
	<link rel="stylesheet" href="css/bootstrap.css">
	<link rel="stylesheet" id="theme-switch" href="css/style.css">


	<!-- Script -->
	<script src="js/jquery.min.js"></script>
	<script src="js/jquery.cookie.js"></script>
	<script>
		if ($.cookie('styleCookie') === 'style-light.css') {
			$('html, body').css('background', '#eeeeee');
		} else if ($.cookie('styleCookie') === 'style.css') {
			$('html, body').css('background', '#222222');
		}

	</script>
	<script src="js/jquery.easing.1.3.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/jquery.waypoints.min.js"></script>
	<script src="js/jquery.flexslider-min.js"></script>
	<script src="js/viewport-units-buggyfill.js"></script>
	<script
		src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCefOgb1ZWqYtj7raVSmN4PL2WkTrc-KyA&sensor=false"></script>
	<script src="js/google_map.js"></script>
	<script src="js/main.js"></script>
	<script src="js/modernizr-2.6.2.min.js"></script>

</head>

<body>

	<!-- Loader -->
	<div class="fh5co-loader"></div>

	<div id="fh5co-page">
		<!-- navbar -->
		<nav id="fh5co-nav" role="navigation">
			<ul>
				<li class="animate-box fh5co-active"><a href="/">Accueil</a></li>
				<!-- <li class="animate-box"><a href="#about">A propos</a></li>
				<li class="animate-box"><a href="#skills">Compétences</a></li> -->
				<li class="animate-box"><a href="#projects">Projets</a></li>
				<li class="animate-box"><a href="#contact">Contact</a></li>
			</ul>
			<a class="style-toggle js-style-toggle" data-style="default" href="#">
				<span class="fh5co-circle"></span>
			</a>
		</nav>
		<!-- Accueil -->
		<header id="fh5co-header" role="banner" class="fh5co-project js-fh5co-waypoint no-border" data-colorbg="#222222"
			data-next="yes">
			<div class="container">
				<div class="fh5co-text-wrap animate-box">
					<div class="fh5co-intro-text">
						<h1>Développeur Fullstack</h1>
						<h2>Spécialisé en <a target="_blank" class="light-a">
								<% skills.forEach((skill, index)=> { %>
									<%= skill.libelle %>
										<%= index !==skills.length - 1 ? ', ' : '' %>
											<% }) %>
							</a>
						</h2>
						<h4>
							<%= user.bio %>
						</h4>
					</div>
				</div>
			</div>
			<div class="btn-next animate-box fh5co-learn-more">
				<a href="#about" class="scroll-btn">
					<span>Qui suis-je?</span>
					<i class="icon-chevron-down"></i>
				</a>
			</div>
		</header>
		<!-- about -->
		<div class="container animate-box" id="skills">
			<h1 class="text-center">A propos</h1>
		</div>
		<div id="about">
			<div class="fh5co-project js-fh5co-waypoint" data-bgcolor="" data-next="yes">
				<div class="container animate-box">
					<div class="fh5co-project-inner row">
						<div class="fh5co-imgs col-md-8 animate-box">
							<div class="img-holder-1 animate-box">
								<img src="images/about_bg.webp" alt="Free HTML5 Bootstrap Template">
							</div>
							<div class="img-holder-2 animate-box">
								<img src="<%= user.image %>" alt="Free HTML5 Bootstrap Template">
							</div>
						</div>
						<div class="fh5co-text col-md-4 animate-box">
							<h2>
								<%= user.nom %>
									<%= user.prenom %>
							</h2>
							<p>
								<%= user.apropos %>.
							</p>
						</div>
					</div>

				</div>
			</div>
		</div>
		<!-- Compétences -->
		<div class="container animate-box" id="skills">
			<h1 class="text-center">Compétences</h1>
		</div>
		<div class="container animate-box row-bottom-padded-sm">
			<div class="row">
				<% skills.forEach(skill=> { %>
					<div class="col-12 col-md-6 col-lg-4 mb-4 animate-box">
						<div class="e-card playing">
							<div class="image"></div>
							<div class="wave"></div>
							<div class="wave"></div>
							<div class="wave"></div>
							<div class="infotop">
								<img style="height: 50px; width: 50px; margin-bottom: 5px;" src="<%= skill.image %>"
									alt="">
								<br>
								<%= skill.libelle %>
									<div class="name">
										<%= skill.description %>
									</div>
							</div>
						</div>
					</div>
					<% }) %>
			</div>
		</div>
		<!-- projet  -->
		<div class="container animate-box" id="projects">
			<h1 class="text-center">Mes Projets</h1>
		</div>
		<div>
			<% projects.forEach((project, index)=> { %>
				<div class="fh5co-project js-fh5co-waypoint <%= index % 2 === 1 ? 'fh5co-reverse' : '' %>"
					data-colorbg="" data-next="yes">
					<div class="container">
						<div class="fh5co-project-inner row">
							<div class="fh5co-imgs col-md-8 <%= index % 2 === 1 ? 'col-md-push-4' : '' %> animate-box">
								<div class="img-holder-1 animate-box">
									<!-- <img src="images/work_2_large.jpg" alt="Free HTML5 Bootstrap Template"> -->
									<img src="<%= project.image1 %>" alt="<%= project.libelle %>">
								</div>
								<div class="img-holder-2 animate-box">
									<!-- <img src="images/work_2_small.jpg" alt="Free HTML5 Bootstrap Template"> -->
									<img src="<%= project.image2 %>" alt="<%= project.libelle %>">
								</div>
							</div>
							<div class="fh5co-text col-md-4 <%= index % 2 === 1 ? 'col-md-pull-8' : '' %> animate-box">
								<h2>
									<%= project.libelle %>
								</h2>
								<p>
									<%= project.description %>
								</p>
								<% if (project.lien_demo) { %>
									<p><a href="<%= project.lien_demo %>" class="btn btn-light btn-outline transition"
											target="_blank">Voir Démo</a></p>
									<% } %>
							</div>
						</div>
					</div>
				</div>
				<% }) %>
		</div>
		<!-- dev stack -->
		<div class="container animate-box">
			<h1 class="text-center">Dev Stack</h1>
		</div>
		<div class="container animate-box row-bottom-padded-sm">
			<div class="col-md-12">
				<div class="flexslider">
					<ul class="slides">
						<% tools.forEach(tool=> { %>
							<li>
								<div class="notification">
									<div class="notiglow"></div>
									<div class="notiborderglow"></div>
									<div class="notititle">
										<img style="height: 50px; width: 50px;" src="<%= tool.image %>" alt="">
										<%= tool.libelle %>
									</div>
									<div class="notibody">
										<%= tool.description %>
									</div>
								</div>
							</li>
							<% }) %>
					</ul>
				</div>
			</div>
		</div>
		<!-- contact -->
		<div class="container animate-box" id="contact">
			<h1 class="text-center">Contact</h1>
		</div>
		<div class="container-grid animate-box">
			<form method="post" action="/contact" class="form animate-box">
				<p class="title">Message </p>
				<p class="message">Vous avez un projet en tête ou une question ? </p>

				<div class="flex">
					<label>
						<input class="input" type="text" name="nom" placeholder="" required="">
						<span>Nom</span>
					</label>

					<label>
						<input class="input" type="email" name="email" placeholder="" required="">
						<span>Email</span>
					</label>
				</div>

				<label>
					<input class="input" type="text" name="sujet" placeholder="" required="">
					<span>Sujet</span>
				</label>
				<label for="">
					<textarea name="message" placeholder="Message" id="" class=""></textarea>
				</label>

				<button class="submit">Envoyer</button>
			</form>

			<div id="address" class="animate-box">
				<h3 class="modern-title">Restons Connectés</h3>

				<div class="contact-info-modern">
					<div class="info-item">
						<i class="fas fa-map-marker-alt modern-icon fa-lg"></i>
						<p>BP <%= user.code_postal %>, <%= user.ville %>, <%= user.pays %>
						</p>
					</div>
					<div class="info-item">
						<i class="fas fa-phone modern-icon fa-lg"></i>
						<p>
							<%= user.telephone %>
						</p>
					</div>
					<div class="info-item">
						<i class="fas fa-envelope modern-icon fa-lg"></i>
						<p>
							<%= user.email %>,
						</p>
					</div>
				</div>

				<h3 class="modern-title">Suivez-moi</h3>
				<div class="social-links-modern">
					<a href="<%= user.linkdin %>" target="_blank" aria-label="LinkedIn de Joel Prince"><i
							class="fab fa-linkedin fa-lg"></i></a>
					<a href="<%= user.github %>" target="_blank" aria-label="GitHub de Joel Prince"><i
							class="fab fa-github fa-lg"></i></a>
					<a href="<%= user.x %>" target="_blank" aria-label="Twitter de Joel Prince"><i
							class="fab fa-x-twitter fa-lg"></i></a>
					<a href="<%= user.instagram %>" target="_blank" aria-label="Instagram de Joel Prince"><i
							class="fab fa-instagram fa-lg"></i></a>
				</div>
			</div>
		</div>
		<!-- footer -->
		<footer id="fh5co-footer" class="js-fh5co-waypoint animate-box">
			<div class="container">
				<div class="row">
					<div class="col-md-12 text-center">
						<p><small>&copy; 2025 <%= user.nom %>
									<%= user.prenom %>. Tous droits réservés.</p>
						<ul class="fh5co-social">
							<li>
								<a href="<%= user.linkdin %>" target="_blank" aria-label="LinkedIn de Joel Prince"><i
										class="fab fa-linkedin fa-lg"></i></a>
							</li>
							<li>
								<a href="<%= user.github %>" target="_blank" aria-label="GitHub de Joel Prince"><i
										class="fab fa-github fa-lg"></i></a>
							</li>
							<li>
								<a href="<%= user.x %>" target="_blank" aria-label="Twitter de Joel Prince"><i
										class="fab fa-x-twitter fa-lg"></i></a>
							</li>
							<li>
								<a href="<%= user.instagram %>" target="_blank" aria-label="Instagram de Joel Prince"><i
										class="fab fa-instagram fa-lg"></i></a>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</footer>

	</div>

</body>

</html>